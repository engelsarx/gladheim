<!--contacto start-->
<section id="contacto" class="contenedor-contacto">
<div class="row">
  <div class="space-blank cuarenta"></div>
  <div class="space-blank cuarenta"></div>
  <div class="small-12 medium-4 column end">
    <h3 class="texto-secondary">¿Cómo podemos ayudarte?</h3>
    <p class="texto-blanco">Envíanos tu solicitud y nos pondremos en contacto contigo.</p>
  </div>
</div>
<div class="space-blank cuarenta"></div>
  <div class="row">
    <section class="small-12 medium-7 large-7 columns">
      <form class="contenedor-contacto__contacto">
        <div class="row">
          <div class="input-field small-12 medium-10 large-8 column">
            <input id="nombre" type="text" class="validate">
            <label for="nombre">Nombre y apellidos *</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field small-12 medium-10 large-8 column">
            <input id="email" type="email" class="validate">
            <label for="email" data-error="Formato incorrecto" data-success="Email correcto">Email *</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field small-12 medium-10 large-8 column">
            <textarea id="comentario" class="materialize-textarea"></textarea>
            <label for="comentario">Comentario *</label>
          </div>
        </div>
        <div class="space-blank ocho"></div>
        <p>
          <input type="checkbox" id="test5" />
          <label for="test5">He leído y acepto la <a>política de privacidad</a> *</label>
        </p>
        <div class="space-blank dieciseis"></div>
        <div class="small-12 medium-8 large-3">
          <button class="button expanded" type="submit">
                                Enviar
                            </button>
        </div>
      </form>
    </section>
    <aside class="contenedor-contacto__redes small-12 medium-5 large-4 large-offset-1 columns">
        <a href=""><i class="fa fa-envelope" aria-hidden="true"></i> info@gladheimlogistics.com</a>
        <a href=""><i class="fa fa-skype" aria-hidden="true"></i> gladheimlogistics</a>
        <a href=""><i class="fa fa-linkedin" aria-hidden="true"></i> Linkedin</a>
    </aside>
  </div>
</section>
